                $('#dbtables').show();
            },
            "select": true,
            "searching": true,  // Enables search box
            "info": true,       // Shows table info
            "paging": true,     // Enables pagination
            "pageLength": 10,   // Default entries per page
            "columns": [
                { "data": "id" },
                { "data": "title" },
                { "data": "slug" },
                {
                    "data": "description",
                    "render": function (data, type, row) {
                        return `<div class="truncate">${data.length > 100 ? data.substr(0, 100) + '…' : data}</div>`;
                    }
                },
                { "data": "created_at" },
                { "data": "updated_at" },
                { "data": "status" },
                {
                    "data": "image",
                    "render": function (data, type, row) {
                        return `<img src="${data}" alt="Image" style="width: 50px; height: auto;">`;
                    }
                },
                { "data": "tags" },
                {
                    "data": null,
                    "render": function (data, type, row) {
                        return `
                            <a href="/admin/blogs/${row.id}" class="btn btn-primary btn-sm edit-btn" data-id="${row.id}">Edit</a>
                            <a href="/admin/blogs/${row.id}" class="btn btn-danger btn-sm delete-btn" data-id="${row.id}">Delete</a>
                        `;
                    }
                }
            ],
            "language": {
                "emptyTable": "No Data to show!"
            },
            "ajax": {
                'type': "GET",
                "url": '/api/blogs',
                "contentType": "application/json; charset=utf-8",
                "dataType": "json",
                "dataSrc": function (json) {
                    console.log(json); 
                    return json.data;
                }
            }
        });

        // Edit button handler
        $(document).on('click', '.edit-btn', function () {
            const id = $(this).data('id');
